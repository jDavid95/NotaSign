

<% if(success === "false") { %>
    <div class="text-center mx-auto alert alert-danger alert-dismissible fade show mt-3 col-4">
        <strong>Error!</strong> Email Address or Password is incorrect.
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
    </div>
<% } %>

<% if(success === "true") { %>
    <div class="text-center mx-auto alert alert-success alert-dismissible fade show mt-3 col-4">
        <strong>Success!</strong> Your account has been succesfully created, login!
        <button id="signupSuccessClose" type="button" class="btn-close" data-bs-dismiss="alert"></button>
    </div>
<% } %>


